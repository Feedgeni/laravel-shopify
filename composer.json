{
    "name": "kudobuzz/laravel-shopify",
    "description": "Laravel shopify package for kudobuzz users",
    "license": "MIT",
    "authors": [
        {
            "name": "kudobuzz"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Kudobuzz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kudobuzz\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "Kudobuzz\\Shopify\\Facades\\Shopify"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
