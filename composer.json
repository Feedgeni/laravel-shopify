{
    "name": "feedgeni/laravel-shopify",
    "description": "Laravel shopify package for FeedGeni users",
    "license": "MIT",
    "authors": [
        {
            "name": "FeedGeni"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "guzzlehttp/guzzle": "7.*",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kudobuzz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kudobuzz\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "Kudobuzz\\Shopify\\Facades\\Shopify"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "require-dev": {
        "rector/rector": "^2.0"
    }
}
